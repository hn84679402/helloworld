public class Cat implements Comparable<Cat>, java.io.Serializable{
	public int hashCode() { //分群
		return age;
	}
	public boolean equals(Object o1) {  //同樣一隻貓不會重復輸入
		return (o1 instanceof Cat) && (this.name.equals(((Cat)o1).name))
				&&(this.age==((Cat)o1).age);
	}
	
	@Override
	public int compareTo(Cat cat2) {        //比照順序
		//return 正-->主角放後面
		//return this.age - cat2.age;  //依age排序(asc)
		//return cat2.age - this.age;  //依age排序(desc)
		
		return this.name.compareTo(cat2.name); //compareTo 比較 照name排序
	}
	private static final long serialVersionUID =1L;
	
	private String name;
	private int age;
	public String toString() {
		return "Cat-"+name+"-"+age;
	}
	public Cat() {
	}
	public Cat(String name,int age) {
		this.name=name;
		this.age=age;
	}
	public void setName(String name) {  //void沒有回傳值
		if(name==null) 
		return;	
		this.name=name;		
	}
	public String getName() {  			
		return name;					//回傳name
	}
	public void setAge(int age) {
		if(age<=0)
			this.age=1;
		else
			this.age=age;
	}
	public int getAge() {
		return age;
	}
	
	
	
}
